<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:KnightElfServer">

    <BitmapImage x:Key="KnightRed" UriSource="/KnightElfServer;component/Resources/knight_red_transparent.png" />
    <BitmapImage x:Key="KnightYellow" UriSource="/KnightElfServer;component/Resources/knight_yellow_transparent.png" />
    <BitmapImage x:Key="KnightGreen" UriSource="/KnightElfServer;component/Resources/knight_green_transparent.png" />
    
    <!-- Converters -->
    <local:StateToIconConverter x:Key="StateToIconConverter"/>
    <local:StateToIconTConverter x:Key="StateToIconTConverter"/>
    <local:StateToAppIconConverter x:Key="StateToAppIconConverter"/>
    
</ResourceDictionary>